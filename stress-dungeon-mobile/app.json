{
  "expo": {
    "name": "stress-dungeon-mobile",
    "slug": "stress-dungeon-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash":         {
      "image": "./assets/images/frame1.png",
      "backgroundColor": "#FDF0D5",
      "resizeMode": "cover"

    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/frame1.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
